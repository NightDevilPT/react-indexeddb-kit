{
	"compilerOptions": {
	  "target": "es6", 
	  "module": "CommonJS",
	  "lib": ["dom", "es6", "es2017"],
	  "declaration": true,
	  "declarationDir": "./dist",
	  "outDir": "./dist",
	  "jsx": "react-jsx",
	  "moduleResolution": "node",
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true,
	  "strict": true,
	  "allowJs": true
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", "dist"]
}
